Section Header
  
  + name := Expanded STACK;
  
  - type := `struct stack`;
  
  - default := `STACK__`:STACK;
  
  - external := 
`
#include <limits.h>
struct stack {
  char x[PTHREAD_STACK_MIN];
};
struct stack STACK__;
`;
  
Section Insert
  
  - parent_object:OBJECT := OBJECT;

Section Public
  
  - set_attr <- `pthread_attr_setstack(&stack_attr, &@Self, sizeof(struct stack))`;