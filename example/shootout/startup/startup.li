Section Header
  
  + name  := STARTUP;
  
Section Inherit
  
  - parent_any:OBJECT := OBJECT;

Section Public
    
  - main <-
  ( + tmp:INTEGER;
    + st:STRING_CONSTANT;
    st := "hello world";
    tmp := st.count;
    `/* @tmp */`;
    st.println;
  );
