lisaac mandelbrot -q -O
gcc -D_ISOC9X_SOURCE -O3 -mfpmath=sse -msse2 -march=pentium4 -ffast-math -funroll-loops -o mandelbrot mandelbrot.c -lm

gcc -D_ISOC9X_SOURCE -O3 -mfpmath=sse -msse2 -march=pentium4 -ffast-math -funroll-loops -o mandelbrot_c mandelbrot_c.c -lm

echo Lisaac: 
time ./mandelbrot 200 > out_li.txt
echo C:
time ./mandelbrot_c 200 > out_c.txt
diff out_li.txt out_c.txt
