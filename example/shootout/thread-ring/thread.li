Section Header
  
  + name := THREAD;
  
  - type := `pthread_t`;
  
  - external := `pthread_t THREAD__;`;
  
Section Insert
  
  - parent_object:OBJECT := OBJECT;
  
Section Public
  
  - create i:INTEGER <- `pthread_create(&@Self, &stack_attr, thread, (void*)@i)`;
  
  - join <- `pthread_join(@Self, NULL)`;
  
