Section Header

  + name := PROTO2;

Section Inherit

  + parent_proto1 :Expanded PROTO1;

Section Public

  - name :STRING_CONSTANT := "PROTO2";

  - println <-
  (
    name.print;
    " var:".print;
    var.print;
    " var2:".print;
    var2.println;
  );

  + var2 :INTEGER;
  - set_var2 i:INTEGER <- (var := i;);
